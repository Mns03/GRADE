#include<stdio.h>
int gcd(int a, int b){
    if (a==0){
        return b;}
    return gcd(b%a,a);
}
int lcm(int x, int y)
{
	int z;
	z=(x*y)/gcd(x,y);
    return z;
}
int main(){
	int a,b,lc;
	printf("Input of first number\n");
	scanf("%d",&a);
	printf("Input of second number\n");
	scanf("%d",&b);
	lc=lcm(a,b);
	printf("LCM of %d and %d is %d",a,b,lc);
	
}
