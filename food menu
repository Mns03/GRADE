#include<stdio.h>
int main(){
	int choice;
	printf("Enter the choices according to following to know  the price\n ");
	printf("Enter 1 for Pizza\n");
	printf("Enter 2 for Burger\n");
	printf("Enter 3 for Pasta\n");
	printf("Enter 4 for French Fries\n");
	printf("Enter 5 for Sandwich\n");
	scanf("%d",&choice);
	switch(choice){
		case 1:
			printf("Food item - Pizza\n");
			printf("Price - Rs 239 ");
			break;
		case 2:
			printf("Food item - Burger\n");
			printf("Price - Rs 129 ");
			break;
		case 3:
			printf("Food item - Pasta\n");
			printf("Price - Rs 179 ");
			break;
		case 4:
			printf("Food item - French Fries\n");
			printf("Price - Rs 99 ");
			break;
		case 5:	
		    printf("Food item - Sandwich\n");
			printf("Price - Rs 149 ");
			break;
		default :
			printf("Wrong choice is entered");
	}
	return 0;
}
